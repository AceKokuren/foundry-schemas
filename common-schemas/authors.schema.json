{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "",
  "type": "array",
  "description": "An array of authors that are identified by ID. Can also optionally add an email and url.",
  "items": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string"
      },
      "email": {
        "type": "string"
      },
      "url": {
        "type": "string"
      }
    }
  },
  "minItems": 1
}